{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "outDir": "../../../dist/out-tsc"
  },
  "include": [
    "/**/*.js",
    "/**/*.jsx",
    "/**/*.ts",
    "/**/*.tsx"
  ],
  "exclude": [
    "jest.config.ts",
    "./**/*.spec.ts",
    "./**/*.test.ts",
    "./**/*.spec.tsx",
    "./**/*.test.tsx",
    "./**/*.spec.js",
    "./**/*.test.js",
    "./**/*.spec.jsx",
    "./**/*.test.jsx",
    "**/*.stories.ts",
    "**/*.stories.js"
  ]
}
