{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/app",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cabal run api",
        "forwardAllArgs": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cabal build api",
        "forwardAllArgs": true
      }
    }
  }
}